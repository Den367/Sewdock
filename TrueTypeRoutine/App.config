<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <system.diagnostics>
    <trace autoflush="true" indentsize="4">
      <listeners>
        <add name="configConsoleListener" type="System.Diagnostics.ConsoleTraceListener"/>
        <add name="FileTrace" type="System.Diagnostics.TextWriterTraceListener" initializeData="Fontloader.log"/>
        <!--add name="DBTrace" type="CommonLibrary.System.Diagnostic.DatabaseTraceListener, CommonLibrary.System.Diagnostic" initializeData="Data Source=(local);Initial Catalog=GISTrace;Persist Security Info=True;User ID=DataTransferUser;Password=qwerty12345"/-->
      </listeners>
    </trace>
  </system.diagnostics>
</configuration>