@model Myembro.ViewModels.DeleteViewModel
@{
    Layout = "~/Views/Shared/SiteLayout.cshtml";
}

@section TitleContent {
    
    Delete @Model.ItemName
        
}


<div id="deletepage">
    <h3>Delete @Model.ItemName</h3>
    <p>Are you sure you want to delete &quot;@Model.ItemDescription&quot;?</p>
    @using (Html.BeginForm()) { 
        @Html.Hidden("returnUrl")
        <input name="submitButton" type="submit" value="Delete" />
     } 
</div>

