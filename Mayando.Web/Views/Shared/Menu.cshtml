@using JelleDruyts.Web.Mvc
@model IEnumerable<Myembro.ViewModels.MenuViewModel>
<div id="menucontainer">
    <ul id="menu" class="breadcrumbs">
        @foreach (var menu in Model)
        {
            
        <li class="@if (menu.Selected)
                   {
                       @Html.Raw("selected")
                   }
                       ">
                @Html.Raw(Html.Link(menu.Url, menu.Title, menu.OpenInNewWindow, menu.ToolTip))
            </li>
        } 
    </ul>
</div>

